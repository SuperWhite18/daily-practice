#include <stdio.h>
#include <math.h>
int main()
{
	int year = 0;
	for(year = 1000;year <= 2000;year++)
	{
		if ((year % 4 == 0) && (year % 100 != 0))//判断闰年的条件：1.每四年一闰年，但是每100年不闰
		{
			printf("%d ", year);
		}
		if (year % 400 == 0)//每400年闰一次
		{
			printf("%d ", year);
		}
	}
	getchar();
	return 0;
}
