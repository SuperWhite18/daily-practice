打印100〜200之间的素数
#include <stdio.h>
#include<math.h>
int main()
{
    int i,j,k=0;
    for(i=100;i<=200;i++)
        for(j=2;j<=sqrt(i);j++)
            if(i%j==0)
                break;
                else if(j>sqrt(i)-1)
                {
                  printf("%d,"i);
                  k++;
                  if(k%5==0)
                     printf("\n");
                }
                else
                   continue;
    return 0;
}




输出乘法口诀表
#include<stdio.h>
int main()
{
	int i, j;
	for (i = 1; i < 10; i++)
	{
		for (j = 1 ;j <= i; j++)
		{
			printf("%d*%d=%d\t", i, j, i * j);
		}
		printf("\n");
	}
	return 0;
}



判断1000年---2000年之间的闰年
#include <stdio.h>
#include <math.h>
int main()
{
	int year = 0;
	for (year = 1000; year <= 2000; year++)
	{
		if ((year % 4 == 0) && (year % 100 != 0))
		{
			printf("%d ", year);
		}
		if (year % 400 == 0)
		{
			printf("%d ", year);
		}
	}
	getchar();
	return 0;
}
